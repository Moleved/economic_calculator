<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ProfitabilityEntity" table="profitability" schema="economic_calculator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="profitFromAllActivities">
            <column name="profit_from_all_activities" sql-type="double" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>