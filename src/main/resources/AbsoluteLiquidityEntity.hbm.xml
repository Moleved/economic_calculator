<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.AbsoluteLiquidityEntity" table="absolute_liquidity" schema="economic_calculator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="shortFinancialInvestments">
            <column name="short_financial_investments" sql-type="double" precision="-1"/>
        </property>
        <property name="funds">
            <column name="funds" sql-type="double" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>